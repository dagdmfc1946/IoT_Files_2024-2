      ('latitud', '7.141879'), ('longitud', '-73.122193'), ('N', '8'), ('finicial',
      '1417000000'), ('fpaso', '1421000000'), ('salto', '40000')], [('0', 'float',
      8)], [], '', ['N', 'finicial', 'fpaso', 'latitud', 'longitud', 'salto', 'url_google_sheet'])
